{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "DatabaseType": "sqlite",
    "StorageSettings": {
        "SharedFilesPath": "data/files",
        "DatabaseFilesPath": "data/db",
        "LocalSharedPaths": null,
        "ExpiredLinkRetentionDays": 30
    },
    "ConnectionStrings": {
        "DefaultConnection": "server=localhost;port=3306;database=mydatabase;user=myuser;password=mypassword"
    },
    "AllowedOrigins": null,
    "Auth": {
        "Issuer": "--filelink--",
        "Audience": "--filelink--",
        "ClientId": "--filelink--",
        "RefreshTokenExpiryInDays": 15,
        "AccessTokenExpiryInMinutes": 15
    },
    "OmdbSettings": {
        "ApiKey": "",
        "BaseUrl": "https://www.omdbapi.com/"
    },
    "RateLimiting": {
        "DownloadPermitLimit": 30,
        "DownloadWindowSeconds": 60,
        "LoginPermitLimit": 5,
        "LoginWindowSeconds": 60
    }
}
